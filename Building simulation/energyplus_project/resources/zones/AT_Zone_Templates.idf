! ========================================================================
! ÖSTERREICHISCHE ZONEN TEMPLATES
! Für EnergyPlus Simulationen - Wiederverwendbare Zone-Ressourcen
! Modularer Aufbau für verschiedene Raumtypen
! ========================================================================

! ========================================================================
! TEMPLATE: WOHNZIMMER (Standard österreichisches EFH)
! ========================================================================

Zone,
    AT_Zone_Wohnzimmer,      !- Name
    0,                       !- Direction of Relative North {deg}
    0,                       !- X Origin {m}
    0,                       !- Y Origin {m}
    0,                       !- Z Origin {m}
    1,                       !- Type
    1,                       !- Multiplier
    autocalculate,           !- Ceiling Height {m}
    autocalculate;           !- Volume {m3}

! Wohnzimmer Grundfläche (5m x 6m = 30m²)
BuildingSurface:Detailed,
    AT_WZ_Boden,             !- Name
    Floor,                   !- Surface Type
    AT_Zwischendecke_Standard, !- Construction Name
    AT_Zone_Wohnzimmer,      !- Zone Name
    OtherSideCoefficients,   !- Outside Boundary Condition
    ,                        !- Outside Boundary Condition Object
    NoSun,                   !- Sun Exposure
    NoWind,                  !- Wind Exposure
    1.0,                     !- View Factor to Ground
    4,                       !- Number of Vertices
    0,0,0,                   !- X,Y,Z ==> Vertex 1 {m}
    0,6,0,                   !- X,Y,Z ==> Vertex 2 {m}
    5,6,0,                   !- X,Y,Z ==> Vertex 3 {m}
    5,0,0;                   !- X,Y,Z ==> Vertex 4 {m}

! Wohnzimmer Decke
BuildingSurface:Detailed,
    AT_WZ_Decke,             !- Name
    Ceiling,                 !- Surface Type
    AT_Zwischendecke_Standard, !- Construction Name
    AT_Zone_Wohnzimmer,      !- Zone Name
    OtherSideCoefficients,   !- Outside Boundary Condition
    ,                        !- Outside Boundary Condition Object
    NoSun,                   !- Sun Exposure
    NoWind,                  !- Wind Exposure
    0,                       !- View Factor to Ground
    4,                       !- Number of Vertices
    0,0,2.7,                 !- X,Y,Z ==> Vertex 1 {m}
    5,0,2.7,                 !- X,Y,Z ==> Vertex 2 {m}
    5,6,2.7,                 !- X,Y,Z ==> Vertex 3 {m}
    0,6,2.7;                 !- X,Y,Z ==> Vertex 4 {m}

! Wohnzimmer Außenwand Süd (mit Fenster)
BuildingSurface:Detailed,
    AT_WZ_Wand_Sued,         !- Name
    Wall,                    !- Surface Type
    AT_Außenwand_WDVS_Standard, !- Construction Name
    AT_Zone_Wohnzimmer,      !- Zone Name
    Outdoors,                !- Outside Boundary Condition
    ,                        !- Outside Boundary Condition Object
    SunExposed,              !- Sun Exposure
    WindExposed,             !- Wind Exposure
    0.5,                     !- View Factor to Ground
    4,                       !- Number of Vertices
    0,0,2.7,                 !- X,Y,Z ==> Vertex 1 {m}
    0,0,0,                   !- X,Y,Z ==> Vertex 2 {m}
    5,0,0,                   !- X,Y,Z ==> Vertex 3 {m}
    5,0,2.7;                 !- X,Y,Z ==> Vertex 4 {m}

! Wohnzimmer Außenwand West
BuildingSurface:Detailed,
    AT_WZ_Wand_West,         !- Name
    Wall,                    !- Surface Type
    AT_Außenwand_WDVS_Standard, !- Construction Name
    AT_Zone_Wohnzimmer,      !- Zone Name
    Outdoors,                !- Outside Boundary Condition
    ,                        !- Outside Boundary Condition Object
    SunExposed,              !- Sun Exposure
    WindExposed,             !- Wind Exposure
    0.5,                     !- View Factor to Ground
    4,                       !- Number of Vertices
    0,0,2.7,                 !- X,Y,Z ==> Vertex 1 {m}
    0,6,2.7,                 !- X,Y,Z ==> Vertex 2 {m}
    0,6,0,                   !- X,Y,Z ==> Vertex 3 {m}
    0,0,0;                   !- X,Y,Z ==> Vertex 4 {m}

! Wohnzimmer Innenwand Nord (zu Küche)
BuildingSurface:Detailed,
    AT_WZ_Wand_Nord,         !- Name
    Wall,                    !- Surface Type
    AT_Innenwand_Ziegel_14cm, !- Construction Name
    AT_Zone_Wohnzimmer,      !- Zone Name
    Surface,                 !- Outside Boundary Condition
    AT_KU_Wand_Sued,         !- Outside Boundary Condition Object (wird später definiert)
    NoSun,                   !- Sun Exposure
    NoWind,                  !- Wind Exposure
    0,                       !- View Factor to Ground
    4,                       !- Number of Vertices
    5,6,2.7,                 !- X,Y,Z ==> Vertex 1 {m}
    0,6,2.7,                 !- X,Y,Z ==> Vertex 2 {m}
    0,6,0,                   !- X,Y,Z ==> Vertex 3 {m}
    5,6,0;                   !- X,Y,Z ==> Vertex 4 {m}

! Wohnzimmer Innenwand Ost (zu Flur)
BuildingSurface:Detailed,
    AT_WZ_Wand_Ost,          !- Name
    Wall,                    !- Surface Type
    AT_Innenwand_Ziegel_14cm, !- Construction Name
    AT_Zone_Wohnzimmer,      !- Zone Name
    Surface,                 !- Outside Boundary Condition
    AT_FL_Wand_West,         !- Outside Boundary Condition Object
    NoSun,                   !- Sun Exposure
    NoWind,                  !- Wind Exposure
    0,                       !- View Factor to Ground
    4,                       !- Number of Vertices
    5,0,2.7,                 !- X,Y,Z ==> Vertex 1 {m}
    5,0,0,                   !- X,Y,Z ==> Vertex 2 {m}
    5,6,0,                   !- X,Y,Z ==> Vertex 3 {m}
    5,6,2.7;                 !- X,Y,Z ==> Vertex 4 {m}

! Wohnzimmer Südfenster (große Fensterfront)
FenestrationSurface:Detailed,
    AT_WZ_Fenster_Sued,      !- Name
    Window,                  !- Surface Type
    AT_Fenster_3fach_Standard, !- Construction Name
    AT_WZ_Wand_Sued,         !- Building Surface Name
    ,                        !- Outside Boundary Condition Object
    0.5,                     !- View Factor to Ground
    ,                        !- Frame and Divider Name
    1,                       !- Multiplier
    4,                       !- Number of Vertices
    0.5,0,2.2,               !- X,Y,Z ==> Vertex 1 {m}
    0.5,0,0.8,               !- X,Y,Z ==> Vertex 2 {m}
    4.5,0,0.8,               !- X,Y,Z ==> Vertex 3 {m}
    4.5,0,2.2;               !- X,Y,Z ==> Vertex 4 {m}

